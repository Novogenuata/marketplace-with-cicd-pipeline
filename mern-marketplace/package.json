{
  "name": "mern-marketplace",
  "version": "2.0.0",
  "description": "A MERN stack based online marketplace application",
  "author": "Shama Hoque",
  "license": "MIT",
  "type": "module",
  "jest": {
    "testEnvironment": "node",
    "moduleNameMapper": {
      "\\.(png|jpg|jpeg|gif|svg)$": "<rootDir>/test/fileMock.js"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/eb-deploy/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shamahoque/mern-marketplace.git"
  },
  "homepage": "https://github.com/shamahoque/mern-marketplace",
  "main": "./dist/server.generated.js",
  "scripts": {
    "development": "nodemon",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack --config webpack.config.client.production.cjs && cross-env NODE_OPTIONS=--openssl-legacy-provider webpack --mode=production --config webpack.config.server.cjs",
    "start": "cross-env NODE_ENV=production node ./dist/server.generated.cjs",
    "test": "jest"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": "6.14.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/node": "^7.28.0",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/register": "^7.28.3",
    "@eslint/js": "^9.39.1",
    "babel-loader": "8.1.0",
    "chai": "^5.2.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "esm": "^3.2.25",
    "file-loader": "6.0.0",
    "globals": "^16.5.0",
    "jest-junit": "^16.0.0",
    "jsdoc": "^4.0.2",
    "jsdoc-babel": "^0.5.0",
    "mocha": "^10.8.2",
    "mongodb-memory-server": "^10.4.1",
    "nodemon": "2.0.2",
    "supertest": "^7.1.4",
    "webpack": "^4.47.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-middleware": "3.7.2",
    "webpack-dev-server": "^4.15.2",
    "webpack-hot-middleware": "2.25.0",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "cross-env": "^10.1.0",
    "@hot-loader/react-dom": "16.13.0",
    "@material-ui/core": "4.9.8",
    "@material-ui/icons": "4.9.1",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-async-errors": "^3.1.1",
    "express-jwt": "5.3.1",
    "formidable": "1.2.2",
    "helmet": "3.22.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.15",
    "mongoose": "5.9.7",
    "query-string": "6.11.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hot-loader": "4.12.20",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-stripe-elements": "6.1.1",
    "request": "2.88.2",
    "socket.io": "2.3.0",
    "socket.io-client": "2.3.0",
    "stripe": "8.38.0"
  }
